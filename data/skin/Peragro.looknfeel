<?xml version="1.0" ?>
<Falagard>

	<!--
    ***************************************************
        Peragro/InvisbleWindow
    ***************************************************
    -->
	<WidgetLook name="Peragro/InvisbleWindow">
		<Property name="FrameEnabled" value="False" />
		<Property name="BackgroundEnabled" value="False" />
		<StateImagery name="Enabled" />
		<StateImagery name="Disabled" />
		<StateImagery name="EnabledFrame" />
		<StateImagery name="DisabledFrame" />
		<StateImagery name="EnabledBackground" />
		<StateImagery name="DisabledBackground" />
	</WidgetLook>
	
	<!--
    ***************************************************
        Peragro/ProgressBar
    ***************************************************
    -->
	<WidgetLook name="Peragro/ProgressBar">
		<PropertyDefinition name="ProgressColours" initialValue="tl: FFFFFFFF tr: FFFFFFFF bl: FFFFFFFF br: FFFFFFFF" />
		<NamedArea name="ProgressArea">
			<Area>
				<Dim type="LeftEdge" >
					<ImageDim imageset="Peragro" image="ProgressBarLeft" dimension="Width" />
				</Dim>
				<Dim type="TopEdge" >
					<AbsoluteDim value="0" />
				</Dim>
				<Dim type="RightEdge" >
					<UnifiedDim scale="1" type="RightEdge">
						<DimOperator op="Subtract">
							<ImageDim imageset="Peragro" image="ProgressBarRight" dimension="Width" />
						</DimOperator>
					</UnifiedDim>
				</Dim>
				<Dim type="Height" >
					<UnifiedDim scale="1" type="Height" />
				</Dim>
			</Area>
		</NamedArea>
		<ImagerySection name="frame">
			<FrameComponent>
				<Area>
					<Dim type="LeftEdge" >
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="TopEdge" >
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="Width" >
						<UnifiedDim scale="1" type="Width" />
					</Dim>
					<Dim type="Height" >
						<UnifiedDim scale="1" type="Height" />
					</Dim>
				</Area>
				<Image type="LeftEdge" imageset="Peragro" image="ProgressBarLeft" />
				<Image type="RightEdge" imageset="Peragro" image="ProgressBarRight" />
				<Image type="Background" imageset="Peragro" image="ProgressBarMiddle" />
			</FrameComponent>
		</ImagerySection>
		<ImagerySection name="progress_lights" >
			<ImageryComponent>
				<Area>
					<Dim type="LeftEdge" >
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="TopEdge" >
						<AbsoluteDim value="0" />
					</Dim>
					<Dim type="Width" >
						<UnifiedDim scale="1" type="Width" />
					</Dim>
					<Dim type="Height" >
						<UnifiedDim scale="1" type="Height" />
					</Dim>
				</Area>
				<Image imageset="Peragro" image="ProgressBarLitSegment" />
				<ColourRectProperty name="ProgressColours" />
				<VertFormat type="Stretched" />
				<HorzFormat type="Tiled" />
			</ImageryComponent>
		</ImagerySection>
		<StateImagery name="Enabled">
			<Layer>
				<Section section="frame" />
			</Layer>
		</StateImagery>
		<StateImagery name="Disabled">
			<Layer>
				<Section section="frame">
					<Colours topLeft="FF7F7F7F" topRight="FF7F7F7F" bottomLeft="FF7F7F7F" bottomRight="FF7F7F7F" />
				</Section>
			</Layer>
		</StateImagery>
		<StateImagery name="EnabledProgress">
			<Layer>
				<Section section="progress_lights" />
			</Layer>
		</StateImagery>
		<StateImagery name="DisabledProgress">
			<Layer>
				<Section section="progress_lights">
					<Colours topLeft="FF7F7F7F" topRight="FF7F7F7F" bottomLeft="FF7F7F7F" bottomRight="FF7F7F7F" />
				</Section>
			</Layer>
		</StateImagery>
	</WidgetLook>

	<!--
    ***************************************************
        Peragro/StaticTextNo
    ***************************************************
    -->
	<WidgetLook name="Peragro/StaticTextNo">
		<Property name="FrameEnabled" value="False" />
		<Property name="BackgroundEnabled" value="False" />
