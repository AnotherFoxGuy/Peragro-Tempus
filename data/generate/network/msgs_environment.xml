<message-implementation>
	<message type="Environment" name="InitTime">
          <event name="environment.inittime" specific="None">
		<uint8>Minute</uint8>
		<uint8>Hour</uint8>
		<uint8>MinutesPerHour</uint8>
		<uint8>HoursPerDay</uint8>
		<uint32>RealPerGame</uint32>
          </event>
	</message>

	<message type="Environment" name="UpdateTime">
          <event name="environment.updatetime" specific="None">
		<uint8>Minute</uint8>
		<uint8>Hour</uint8>
          </event>
        </message>

        <message type="Environment" name="Weather">
          <event name="environment.weather" specific="None">
                <uint8>WeatherId</uint8>
          </event>
	</message>

</message-implementation>
