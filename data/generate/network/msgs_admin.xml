<message-implementation>
	<message type="Admin" name="RemoveAll">
		<string>DataType</string>
	</message>

	<message type="Admin" name="CreateSector">
		<uint16>SectorId</uint16>
		<string>Name</string>
		<string>Region</string>
	</message>

	<message type="Admin" name="CreateItem">
		<uint32>ItemId</uint32>
		<string>Name</string>
		<string>Icon</string>
		<string>Description</string>
		<string>File</string>
		<string>Mesh</string>
		<float>Weight</float>
		<string>EquipType</string>
	</message>

	<message type="Admin" name="CreateNpc">
		<string>Name</string>
                <string>FileName</string>
		<string>Mesh</string>
		<vector3f>Pos</vector3f>
		<float>Rotation</float>
		<uint16>SectorId</uint16>
		<string>Race</string>
		<colour24>HairColour</colour24>
		<colour24>SkinColour</colour24>
		<colour24>DecalColour</colour24>
		<string>Ai</string>
		<list name="AiSetting">
			<string>Key</string>
			<string>Value</string>
		</list>
		<list name="Inventory">
			<uint8>SlotId</uint8>
			<uint32>ItemId</uint32>
			<uint32>Variation</uint32>
		</list>
	</message>

	<message type="Admin" name="CreateSpawnPoint">
		<uint32>ItemId</uint32>
		<uint32>Variation</uint32>
		<vector3f>Pos</vector3f>
		<uint16>SectorId</uint16>
		<uint32>Interval</uint32>
	</message>

	<message type="Admin" name="SpawnItem">
		<uint32>ItemId</uint32>
		<uint32>Variation</uint32>
		<vector3f>Pos</vector3f>
		<uint16>SectorId</uint16>
	</message>

	<message type="Admin" name="SpawnMount">
		<string>Name</string>
		<string>Mesh</string>
		<vector3f>Pos</vector3f>
		<float>Rotation</float>
		<uint16>SectorId</uint16>
	</message>

	<message type="Admin" name="SpawnDoor">
		<uint16>DoorId</uint16>
		<string>Name</string>
		<string>Mesh</string>
		<uint16>SectorId</uint16>
		<vector3f>Pos</vector3f>
		<bool>IsOpen</bool>
		<bool>IsLocked</bool>
		<string>Animation</string>
	</message>
	
	<message type="Admin" name="RemoveSpawnedEntity">
		<uint32>EntityId</uint32>
	</message>
	
	<message type="Admin" name="ToggleFlashStep"></message>

	<message type="Admin" name="CreateZone">
		<uint16>ZoneId</uint16>
		<string>ZoneType</string>
		<list name="Nodes">
			<float>X</float>
			<float>Z</float>
		</list>
	</message>

</message-implementation>
