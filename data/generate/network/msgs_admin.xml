<message-implementation>
	<message type="Admin" name="CreateSector">
		<uint16>SectorId</uint16>
		<string>Name</string>
		<string>Region</string>
	</message>

	<message type="Admin" name="CreateItem">
		<uint32>ItemId</uint32>
		<string>Name</string>
		<string>Icon</string>
		<string>Description</string>
		<string>File</string>
		<string>Mesh</string>
		<float>Weight</float>
		<string>EquipType</string>
	</message>

	<message type="Admin" name="RemoveAll">
		<string>DataType</string>
	</message>

	<message type="Admin" name="CreateNpc">
		<string>Name</string>
		<string>Mesh</string>
		<vector3f>Pos</vector3f>
		<float>Rotation</float>
		<uint16>SectorId</uint16>
		<string>Race</string>
		<colour24>HairColour</colour24>
		<colour24>SkinColour</colour24>
		<colour24>DecalColour</colour24>
		<uint16>Dialog</uint16>
		<string>Ai</string>
		<list name="AiSetting">
			<string>Key</string>
			<string>Value</string>
		</list>
	</message>

</message-implementation>
