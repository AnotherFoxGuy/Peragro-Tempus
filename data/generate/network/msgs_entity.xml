<message-implementation>
	<message type="Entity" name="AddNpcEntity">
		<string>Name</string>
		<string>Mesh</string>
		<uint16>MeshId</uint16>
		<vector3f>Pos</vector3f>
		<float>Rotation</float>
		<uint16>SectorId</uint16>
		<uint32>EntityId</uint32>
		<uint8>PoseId</uint8>
		<list name="Equipment">
			<uint32>ItemId</uint32>
			<uint32>Variation</uint32>
		</list>
	</message>

	<message type="Entity" name="AddItemEntity">
		<uint32>ItemId</uint32>
		<uint32>Variation</uint32>
		<vector3f>Pos</vector3f>
		<uint16>SectorId</uint16>
		<uint32>EntityId</uint32>
	</message>

	<message type="Entity" name="AddDoorEntity">
		<uint16>DoorId</uint16>
		<bool>IsOpen</bool>
		<bool>IsLocked</bool>
		<uint32>EntityId</uint32>
		<string>Name</string>
		<string>Mesh</string>
		<uint16>SectorId</uint16>
		<string>Animation</string>
	</message>

	<message type="Entity" name="AddPlayerEntity">
		<string>Name</string>
		<string>Mesh</string>
		<uint16>MeshId</uint16>
		<vector3f>Pos</vector3f>
		<float>Rotation</float>
		<colour24>HairColour</colour24>
		<colour24>SkinColour</colour24>
		<colour24>DecalColour</colour24>
		<uint16>SectorId</uint16>
		<uint32>EntityId</uint32>
		<uint8>PoseId</uint8>
		<list name="Equipment">
			<uint32>ItemId</uint32>
			<uint32>Variation</uint32>
		</list>
	</message>

	<message type="Entity" name="Remove">
		<uint32>EntityId</uint32>
	</message>

	<message type="Entity" name="Move">
		<float>Turn</float>
		<float>Walk</float>
		<uint32>EntityId</uint32>
		<bool>Run</bool>
		<bool>Jump</bool>
	</message>

	<message type="Entity" name="MoveRequest">
		<uint8>Turn</uint8>
		<uint8>Walk</uint8>
		<bool>Run</bool>
		<bool>Jump</bool>
	</message>

	<message type="Entity" name="Pose">
		<uint32>EntityId</uint32>
		<uint8>PoseId</uint8>
	</message>

	<message type="Entity" name="PoseRequest">
		<uint8>PoseId</uint8>
	</message>

	
	<message type="Entity" name="PickRequest">
		<uint32>ItemEntityId</uint32>
		<uint8>Slot</uint8>
	</message>

	<message type="Entity" name="PickResponse">
		<uint32>ItemId</uint32>
		<uint32>Variation</uint32>
		<uint8>SlotId</uint8>
		<string>Error</string> <!-- null if successful -->
	</message>

	<message type="Entity" name="DropRequest">
		<uint8>Slot</uint8>
		<uint32>InventoryId</uint32>
	</message>

	<message type="Entity" name="DropResponse">
		<uint8>SlotId</uint8>
		<string>Error</string> <!-- null if successful -->
	</message>

	<message type="Entity" name="InventoryList">
		<uint32>InventoryId</uint32>
		<list name="Inventory">
			<uint32>ItemId</uint32>
			<uint32>Variation</uint32>
			<uint8>SlotId</uint8>
		</list>
	</message>

	<message type="Entity" name="StatsList">
		<list name="Stats">
			<uint16>StatId</uint16>
			<string>Name</string>
			<uint16>Level</uint16>
		</list>
	</message>

	<message type="Entity" name="StatsChange">
		<uint16>StatId</uint16>
		<uint16>Level</uint16>
	</message>

	<message type="Entity" name="SkillsList">
		<list name="Skills">
			<uint16>SkillId</uint16>
			<string>Name</string>
		</list>
	</message>

	<message type="Entity" name="Equip">
		<uint32>EntityId</uint32>
		<uint32>ItemId</uint32>
		<uint32>Variation</uint32>
		<uint8>SlotId</uint8>
	</message>

	<message type="Entity" name="MoveTo">
		<vector3f>From</vector3f>
		<vector3f>To</vector3f>
		<float>Speed</float>
		<uint32>EntityId</uint32>
		<bool>Run</bool>
	</message>

	<message type="Entity" name="MoveToRequest">
		<vector3f>To</vector3f>
		<bool>Run</bool>
	</message>

	<message type="Entity" name="Relocate"></message>

	<message type="Entity" name="ToggleFlashStep"></message>

	<message type="Entity" name="Teleport">
		<uint32>EntityId</uint32>
		<vector3f>Pos</vector3f>
		<uint16>SectorId</uint16>
	</message>
	
	<message type="Entity" name="DrUpdateRequest">
		<vector3f>Pos</vector3f>
		<float>Rotation</float>
		<uint16>SectorId</uint16>
	</message>

	<message type="Entity" name="DrUpdate">
		<vector3f>Pos</vector3f>
		<float>Rotation</float>
		<uint16>SectorId</uint16>
		<uint32>EntityId</uint32>
	</message>

	<message type="Entity" name="InventoryMoveItemRequest">
		<uint8>OldSlot</uint8>
		<uint32>OldInventoryId</uint32>
		<uint8>NewSlot</uint8>
		<uint32>NewInventoryId</uint32>
	</message>
	
	<message type="Entity" name="InventoryMoveItem">
		<uint8>OldSlot</uint8>
		<uint32>OldInventoryId</uint32>
		<uint8>NewSlot</uint8>
		<uint32>NewInventoryId</uint32>
		<string>Error</string> <!-- null if successful -->
	</message>

	<message type="Entity" name="AddMountEntity">
		<string>Name</string>
		<string>Mesh</string>
		<uint16>MeshId</uint16>
		<vector3f>Pos</vector3f>
		<float>Rotation</float>
		<uint16>SectorId</uint16>
		<uint32>EntityId</uint32>
		<uint32>InventoryId</uint32>
	</message>

	<message type="Entity" name="MountRequest">
		<uint32>MountEntityId</uint32>
	</message>

	<message type="Entity" name="Mount">
		<uint32>PlayerEntityId</uint32>
		<uint32>MountEntityId</uint32>
		<bool>CanControl</bool>
	</message>

	<message type="Entity" name="UnmountRequest">
		<uint32>MountEntityId</uint32>
	</message>

	<message type="Entity" name="Unmount">
		<uint32>PlayerEntityId</uint32>
		<uint32>MountEntityId</uint32>
	</message>

	<message type="Entity" name="SpawnItem">
		<uint32>ItemId</uint32>
		<uint32>Variation</uint32>
		<vector3f>Pos</vector3f>
		<uint16>SectorId</uint16>
	</message>

	<message type="Entity" name="SpawnMount">
		<string>Name</string>
		<string>Mesh</string>
		<vector3f>Pos</vector3f>
		<float>Rotation</float>
		<uint16>SectorId</uint16>
	</message>

	<message type="Entity" name="SpawnDoor">
		<uint16>DoorId</uint16>
		<string>Name</string>
		<string>Mesh</string>
		<uint16>SectorId</uint16>
		<vector3f>Pos</vector3f>
		<bool>IsOpen</bool>
		<bool>IsLocked</bool>
		<string>Animation</string>
	</message>
	
</message-implementation>
